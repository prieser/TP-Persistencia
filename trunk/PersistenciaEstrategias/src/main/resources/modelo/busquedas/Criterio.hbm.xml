<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="modelo.busquedas.Criterio" table="Criterios">
		<id name="idCriterio" type="java.lang.Integer">
			<column name="ID_CRITERIO" />
			<generator class="identity" />
		</id>
		
		<property name="clave" type="string">
			<column name="CLAVE" length="20" not-null="true" unique="false" />
		</property>

		<property name="valor" type="string">
			<column name="VALOR" length="20" not-null="true" unique="false" />
		</property>
		
	</class>
</hibernate-mapping>
